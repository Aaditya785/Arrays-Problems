
public class Solution {  

    public static void selectionSort(int[] arr) {
    	//Your code goes here
        int n = arr.length, temp = 0;
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if(arr[i]>arr[j])
                {
                    temp = arr[i];
                    arr[i] = arr[j];
                    arr[j] = temp;
                }
            }
        }
    }   

}
