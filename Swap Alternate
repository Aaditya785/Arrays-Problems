
public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	//Your code goes here
        int temp = 0, n = arr.length, l = 0;
        
        if(n%2==0)
        {
            l = n-2;
        }
        else
        {
            l = n-3;
        }
        for(int i=0;i<=l;i+=2)
        {
            temp = arr[i];
            arr[i] = arr[i+1];
            arr[i+1] = temp;
        }
        
    }
}
